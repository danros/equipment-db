<tr<%=raw " class=\"#{device_row.highlight_style}\"" if device_row.highlight_style %>>
  <td><a href="<%=device_row.path %>"><%=device_row.name %></a></td>
  <td><%=device_row.model %></td>
  <td><%=device_row.asset_code %></td>
  <td>
    <% if device_row.emphasize_status %>
      <strong><%=device_row.status %></strong>
    <% else %>
      <%=device_row.status %>
    <% end %>
  </td>
  <td><span style="color:#<%=device_row.training_category_rgb %>"><%=device_row.training_category %></span></td>
  <td>
    <%= raw " <a href=\"#{device_row.wiki_url}\">wiki</a>" if !device_row.wiki_url.blank? %>
  </td>
</tr>
